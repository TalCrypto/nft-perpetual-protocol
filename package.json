{
  "name": "@tribe3-xyz/protocol",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "graph-codegen": "cd tribe3-perp-subgraph && graph codegen",
    "graph-build": "cd tribe3-perp-subgraph && graph build",
    "graph-test": "graph test",
    "graph-local": "docker-compose up",
    "graph-local-clean": "docker-compose down -v && docker-compose rm -v && rm -rf data/ipfs data/postgres",
    "create-local": "graph create --node http://127.0.0.1:8020 tribe3-perp",
    "deploy-local": "cd tribe3-perp-subgraph && graph deploy --ipfs http://127.0.0.1:5001 --node http://127.0.0.1:8020 tribe3-perp",
    "hardhat-local": "hardhat node --hostname 0.0.0.0"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.33.0",
    "@graphprotocol/graph-ts": "^0.27.0",
    "@graphprotocol/hardhat-graph": "^0.1.0-alpha.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.2",
    "@nomiclabs/hardhat-ethers": "^2.0.6",
    "@nomiclabs/hardhat-etherscan": "^3.1.0",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@openzeppelin/hardhat-upgrades": "^1.19.0",
    "@openzeppelin/test-helpers": "^0.5.15",
    "@typechain/ethers-v5": "^10.1.0",
    "@typechain/hardhat": "^6.1.2",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.0.0",
    "chai": "^4.3.6",
    "dotenv": "^16.0.1",
    "ethereum-waffle": "^3.4.4",
    "ethers": "^5.6.9",
    "fs": "^0.0.1-security",
    "hardhat": "^2.9.9",
    "hardhat-gas-reporter": "^1.0.8",
    "matchstick-as": "^0.5.0",
    "prettier": "^2.7.1",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "ts-node": "^10.8.1",
    "typechain": "^8.1.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.6.0",
    "@openzeppelin/contracts-upgradeable": "^4.6.0",
    "graphql": "^16.5.0",
    "graphql-request": "^4.3.0"
  }
}
